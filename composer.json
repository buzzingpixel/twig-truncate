{
    "name": "buzzingpixel/twig-truncate",
    "version": "1.0.0",
    "description": "A Twig Truncate filter",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^2.5",
        "timostamm/text-truncation": "dev-master",
        "buzzingpixel/corbomite-di": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\twigtruncate\\": "src"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "twigExtensions": [
            "buzzingpixel\\twigtruncate\\TruncateTwigExtension"
        ]
    }
}
